            "content": {
              "aplication/json": {
                "schema": {
                  "$ref": "#/components/schemas/InvalidTokenError"
                }
              }
            }
          },
          "403": {
            "description": "Usuário tentando acessar informações de outro usuário sem ter poderes de administrador",
            "content": {
